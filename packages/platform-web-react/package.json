{
  "name": "@shuvi/platform-web-react",
  "version": "0.0.1-pre.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/platform-web-react"
  },
  "author": "liximomo",
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "shuvi-app"
  ],
  "scripts": {
    "dev": "run-p watch:*",
    "watch:app": "tsc -p tsconfig.build.app.json -w",
    "watch:node.esm": "tsc -p tsconfig.build.node.esm.json -w",
    "watch:node.cjs": "tsc -p tsconfig.build.node.cjs.json -w",
    "prebuild": "rimraf lib esm shuvi-app",
    "build": "run-p build:*",
    "build:app": "tsc -p tsconfig.build.app.json",
    "build:node.esm": "tsc -p tsconfig.build.node.esm.json",
    "build:node.cjs": "tsc -p tsconfig.build.node.cjs.json"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@next/react-refresh-utils": "10.0.5",
    "@shuvi/app": "0.0.1-pre.2",
    "@shuvi/router-react": "0.0.1-pre.2",
    "@shuvi/service": "0.0.1-pre.2",
    "@shuvi/shared": "0.0.1-pre.2",
    "@shuvi/utils": "0.0.1-pre.2",
    "react": "16.13.0",
    "react-app-polyfill": "2.0.0",
    "react-dom": "16.13.0",
    "react-refresh": "0.9.0",
    "use-subscription": "1.4.1"
  }
}
