{
  "name": "@shuvi/runtime-react",
  "version": "0.0.1-beta.26",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liximomo/shuvi.git",
    "directory": "packages/runtime-react"
  },
  "author": "liximomo",
  "license": "MIT",
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "files": [
    "lib",
    "es",
    "style",
    "client-env.d.ts",
    "index.d.ts",
    "index.js",
    "shuvi-app.d.ts"
  ],
  "scripts": {
    "dev": "run-p watch:*",
    "watch:es": "tsc -p tsconfig.build.json -w -m esnext --outDir es",
    "watch:lib": "tsc -p tsconfig.build.json -w -m commonjs --outDir lib",
    "prebuild": "rimraf lib es",
    "build": "run-p build:*",
    "build:es": "tsc -p tsconfig.build.json -m esnext --outDir es",
    "build:lib": "tsc -p tsconfig.build.json -m commonjs --outDir lib"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@shuvi/utils": "^0.0.1-beta.26",
    "enhanced-resolve": "5.0.0-beta.4",
    "history": "4.10.1",
    "react": "16.13.0",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.13.0",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "use-subscription": "^1.3.0"
  },
  "devDependencies": {
    "@shuvi/types": "^0.0.1-beta.26",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.3"
  },
  "peerDependencies": {
    "@shuvi/app": "^0.0.1-alpha.1"
  }
}
