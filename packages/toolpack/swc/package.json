{
  "name": "@shuvi/swc",
  "version": "1.0.0-rc.2",
  "private": true,
  "scripts": {
    "build-native": "napi build --platform -p shuvi-swc-napi --cargo-name shuvi_swc_napi native --features plugin"
  },
  "napi": {
    "name": "shuvi-swc",
    "triples": {
      "defaults": true,
      "additional": [
        "i686-pc-windows-msvc",
        "aarch64-unknown-linux-gnu",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "x86_64-unknown-freebsd",
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  }
}
