{
  "name": "@shuvi/plugins",
  "version": "0.0.1-pre.10",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/plugins"
  },
  "author": "liximomo",
  "license": "MIT",
  "files": [
    "lib",
    "esm"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "run-p watch:*",
    "watch:esm": "tsc -p tsconfig.build.esm.json -w",
    "watch:cjs": "tsc -p tsconfig.build.cjs.json -w",
    "prebuild": "rimraf lib esm",
    "build": "run-p build:*",
