    "redux": "4.1.2",
    "@shuvi/utils": "0.0.1-pre.10"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-test-renderer": "^16.9.2",
    "react-test-renderer": "^16.13.1"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
