{
  "name": "@shuvi/router-react",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/router-react"
  },
  "author": "liximomo",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "dev": "run-p watch:*",
    "watch:esm": "tsc -p tsconfig.build.json -m esnext --outDir esm -w",
    "watch:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib -w",
    "prebuild": "rimraf lib esm",
    "build": "run-p build:*",
    "build:esm": "tsc -p tsconfig.build.json -m esnext --outDir esm",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@shuvi/router": "1.0.4",
    "@shuvi/platform-shared": "1.0.4",
    "@shuvi/utils": "1.0.4",
    "prop-types": "^15.8.1",
    "use-sync-external-store": "1.1.0"
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.5",
    "@types/react": "18.0.9",
    "@types/react-test-renderer": "18.0.0",
    "@types/use-sync-external-store": "0.0.3",
    "react-test-renderer": "^18.1.0",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
