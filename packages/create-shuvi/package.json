{
  "name": "create-shuvi",
  "version": "1.0.18",
  "keywords": [
    "react",
    "shuvi",
    "shuvi.js"
  ],
  "description": "Create Shuvi.js-powered React apps with one command",
  "license": "MIT",
  "bin": {
    "create-shuvi": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prebuild": "rimraf ./dist/",
    "build": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register"
  },
  "devDependencies": {
    "@types/async-retry": "1.3.0",
    "@types/cross-spawn": "6.0.0",
    "@types/node": "^12.6.8",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.34.0",
    "async-retry": "1.3.3",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "prompts": "2.1.0",
    "rimraf": "3.0.2",
    "tar": "4.4.10",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
