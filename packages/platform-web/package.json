{
  "name": "@shuvi/platform-web",
  "version": "0.0.1-pre.10",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/platform-web"
  },
  "author": "liximomo",
  "license": "MIT",
  "sideEffects": [
    "**/shuvi-app/entry/client/**"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "shuvi-app"
  ],
  "scripts": {
    "dev": "run-p watch:*",
    "watch:app": "tsc -p tsconfig.build.browser.json -w",
    "watch:node.esm": "tsc -p tsconfig.build.node.json -m esnext --outDir esm -w",
    "watch:node.cjs": "tsc -p tsconfig.build.node.json -m commonjs --outDir lib -w",
    "prebuild": "rimraf lib esm shuvi-app",
    "build": "run-p build:*",
    "build:app": "tsc -p tsconfig.build.browser.json",
    "build:node.esm": "tsc -p tsconfig.build.node.json -m esnext --outDir esm",
    "build:node.cjs": "tsc -p tsconfig.build.node.json -m commonjs --outDir lib"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@next/react-refresh-utils": "10.0.5",
    "@shuvi/hook": "0.0.1-pre.10",
    "@shuvi/platform-shared": "0.0.1-pre.10",
    "@shuvi/plugins": "0.0.1-pre.10",
    "@shuvi/router": "0.0.1-pre.10",
    "@shuvi/router-react": "0.0.1-pre.10",
    "@shuvi/runtime": "0.0.1-pre.10",
    "@shuvi/service": "0.0.1-pre.10",
    "@shuvi/shared": "0.0.1-pre.10",
    "@shuvi/toolpack": "0.0.1-pre.10",
    "@shuvi/utils": "0.0.1-pre.10",
    "@shuvi/redox": "0.0.1-pre.6",
    "@shuvi/redox-react": "0.0.1-pre.6",
    "content-type": "^1.0.4",
    "cookie": "^0.4.1",
    "ejs": "3.1.5",
    "fs-extra": "9.0.1",
    "node-mocks-http": "^1.11.0",
    "raw-body": "^2.4.1",
    "react-app-polyfill": "2.0.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-refresh": "0.8.3",
    "use-subscription": "1.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.2",
    "@types/content-type": "^1.1.5",
    "@types/cookie": "^0.4.1",
    "@types/ejs": "^3.1.0",
    "@types/raw-body": "^2.3.0",
    "@types/webpack-env": "^1.16.2",
    "webpack": "5.36.0"
  }
}
