{
  "name": "@shuvi/platform-web",
  "version": "0.0.1-pre.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/platform-web"
  },
  "author": "liximomo",
  "license": "MIT",
  "sideEffects": [
    "**/shuvi-app/entry/client/**"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "shuvi-app"
  ],
  "scripts": {
    "dev": "run-p watch:*",
    "watch:app": "tsc -p tsconfig.build.app.json -w",
    "watch:node.esm": "tsc -p tsconfig.build.node.esm.json -w",
    "watch:node.cjs": "tsc -p tsconfig.build.node.cjs.json -w",
    "prebuild": "rimraf lib esm shuvi-app",
    "build": "run-p build:*",
    "build:app": "tsc -p tsconfig.build.app.json",
    "build:node.esm": "tsc -p tsconfig.build.node.esm.json",
    "build:node.cjs": "tsc -p tsconfig.build.node.cjs.json"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@shuvi/app": "0.0.1-pre.2",
    "@shuvi/platform-core": "0.0.1-pre.2",
    "@shuvi/platform-web-react": "0.0.1-pre.2",
    "@shuvi/service": "0.0.1-pre.2",
    "@shuvi/shared": "0.0.1-pre.2"
  }
}
