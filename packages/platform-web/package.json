{
  "name": "@shuvi/platform-web",
  "version": "1.0.0-rc.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuvijs/shuvi.git",
    "directory": "packages/platform-web"
  },
  "author": "liximomo",
  "license": "MIT",
  "sideEffects": [
    "**/shuvi-app/entry/client/**"
  ],
  "files": [
    "lib",
    "esm",
    "shuvi-app-extensions.d.ts"
  ],
  "exports": {
    ".": {
      "default": "./lib/node/index.js"
    },
    "./node": {
      "default": "./lib/node/index.js"
    },
    "./node/*": {
      "default": "./lib/node/*.js"
    },
    "./shared": {
      "default": "./esm/shared/index.js"
    },
    "./shared/*": {
      "default": "./esm/shared/*.js"
    },
    "./shuvi-app": {
      "default": "./esm/shuvi-app/index.js"
    },
    "./shuvi-app/*": {
      "default": [
        "./esm/shuvi-app/*.js",
        "./esm/shuvi-app/*.jsx"
      ]
    },
    "./shuvi-app-extensions.d.ts": "./shuvi-app-extensions.d.ts"
  },
  "scripts": {
    "dev": "run-p watch:*",
    "watch:esm": "tsc -p tsconfig.build.browser.json -w",
    "watch:cjs": "tsc -p tsconfig.build.node.json -w",
    "prebuild": "rimraf lib esm types",
    "build": "run-p build:*",
    "build:esm": "tsc -p tsconfig.build.browser.json",
    "build:cjs": "tsc -p tsconfig.build.node.json"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@next/react-refresh-utils": "12.1.6",
    "@shuvi/hook": "1.0.0-rc.1",
    "@shuvi/platform-shared": "1.0.0-rc.1",
    "@shuvi/redox": "0.0.3",
    "@shuvi/redox-react": "0.0.3",
    "@shuvi/router": "1.0.0-rc.1",
    "@shuvi/router-react": "1.0.0-rc.1",
    "@shuvi/runtime": "1.0.0-rc.1",
    "@shuvi/service": "1.0.0-rc.1",
    "@shuvi/shared": "1.0.0-rc.1",
    "@shuvi/toolpack": "1.0.0-rc.1",
    "@shuvi/utils": "1.0.0-rc.1",
    "content-type": "1.0.4",
    "cookie": "0.4.1",
    "ejs": "3.1.5",
    "fs-extra": "9.0.1",
    "node-mocks-http": "1.11.0",
    "raw-body": "2.4.1",
    "react": "18.1.0",
    "react-app-polyfill": "2.0.0",
    "react-dom": "18.1.0",
    "react-refresh": "0.12.0",
    "use-sync-external-store": "1.1.0"
  },
  "devDependencies": {
    "@shuvi/redox": "0.0.3",
    "@testing-library/react": "^13.2.0",
    "@types/content-type": "^1.1.5",
    "@types/cookie": "^0.4.1",
    "@types/ejs": "^3.1.0",
    "@types/raw-body": "^2.3.0",
    "webpack": "5.73.0"
  }
}
