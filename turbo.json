{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build-native": {
      "dependsOn": ["^build-native"],
      "outputs": ["native/*.node"]
    },
    "build-native-no-plugin": {
      "dependsOn": ["^build-native-no-plugin"],
      "outputs": ["native/*.node"]
    }
  }
}
